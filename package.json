    "body-scroll-lock": "^3.1.5",
    "tailwindcss": "^2.2.15",
    "tailwindcss-autofill": "^0.1.0",
    "tailwindcss-shadow-fill": "^0.1.0",
    "tailwindcss-text-fill": "^0.1.2",
    "vue": "^2.6.14",
    "webfontloader": "github:creativedotdesign/webfontloader#feature/google-fonts-v2"
  }
}
